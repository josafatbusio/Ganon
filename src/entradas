          trim(to_char(NVL(v_exis_sub,0),'999999999999.999'))||'|'||
          v_linea8 ||chr(10)||chr(13)
          );

  v_cont_exis := v_cont_exis + 1;

-- end if;

end loop;
close c_existencias;
--layout clientes

reg_clientes:=null;

open c_clientes;
loop
