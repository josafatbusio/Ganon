reg_tabla_clientes.TIPO:=reg_clientes.TIPO;
--reg_cliente.TIPOCTE:=reg_clientes.COLONIA;
                --reg_cliente.GIRO:=reg_clientes.GIRO;
                --reg_cliente.CLEDO:=reg_clientes.COLONIA;
reg_tabla_clientes.DIAS:=reg_clientes.DIAS;
