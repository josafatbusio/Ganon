close c_clientes;

if v_cont_exis = 0 then
--    dbms_output.put_line('No se genera archivo de existencias');            
UTL_FILE.FCLOSE(v_oh_file);
rollback;
UTL_FILE.FREMOVE(v_path,v_file);
ELSE
